<?xml version="1.0" encoding="utf-8"?>

<ScrollView  xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:fillViewport="true">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="#D7D0FF"
        tools:context=".LoginActivity">

        <TextView
            android:id="@+id/textView5"
            android:layout_width="373dp"
            android:layout_height="65dp"
            android:layout_marginStart="19dp"
            android:layout_marginTop="315dp"
            android:background="@drawable/rect_1"
            android:fontFamily="@font/latobold"
            android:gravity="center"
            android:text="Login"
            android:textAlignment="center"
            android:textColor="#FFFFFF"
            android:textSize="23sp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <EditText
            android:id="@+id/textView6"
            android:layout_width="373dp"
            android:layout_height="65dp"
            android:layout_marginStart="19dp"
            android:layout_marginTop="104dp"
            android:layout_marginEnd="19dp"
            android:layout_marginBottom="722dp"
            android:background="@drawable/rect3"
            android:drawableStart="@drawable/email"
            android:drawablePadding="15dp"
            android:fontFamily="@font/latoregular"
            android:hint="Enter your email"
            android:inputType="textEmailAddress"
            android:paddingStart="25dp"
            android:paddingEnd="25dp"
            android:textAlignment="textStart"
            android:textColor="#1D212D"
            android:textSize="20sp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:id="@+id/textView9"
            android:layout_width="373dp"
            android:layout_height="65dp"
            android:layout_marginStart="19dp"
            android:layout_marginTop="499dp"
            android:layout_marginEnd="19dp"
            android:background="@drawable/rect3"
            android:drawableStart="@drawable/google"
            android:fontFamily="@font/latobold"
            android:gravity="center"
            android:paddingStart="25dp"
            android:paddingEnd="40dp"
            android:text="Sign in with Google"
            android:textAlignment="center"
            android:textColor="#1D212D"
            android:textSize="20sp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:id="@+id/textView10"
            android:layout_width="373dp"
            android:layout_height="65dp"
            android:layout_marginStart="19dp"
            android:layout_marginTop="577dp"
            android:layout_marginEnd="19dp"
            android:background="@drawable/rect3"
            android:drawableStart="@drawable/apple"
            android:fontFamily="@font/latobold"
            android:gravity="center"
            android:paddingStart="25dp"
            android:paddingEnd="40dp"
            android:text="Sign in with Apple"
            android:textAlignment="center"
            android:textColor="#1D212D"
            android:textSize="20sp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:id="@+id/textView12"
            android:layout_width="373dp"
            android:layout_height="65dp"
            android:layout_marginStart="19dp"
            android:layout_marginTop="730dp"
            android:background="@drawable/rect3"
            android:drawableStart="@drawable/call"
            android:fontFamily="@font/latobold"
            android:gravity="center"
            android:paddingStart="25dp"
            android:paddingEnd="40dp"
            android:text="Sign in with Number"
            android:textAlignment="center"
            android:textColor="#1D212D"
            android:textSize="20sp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:id="@+id/textView13"
            android:layout_width="373dp"
            android:layout_height="65dp"
            android:layout_marginStart="19dp"
            android:layout_marginTop="653dp"
            android:background="@drawable/rect3"
            android:drawableStart="@drawable/faceb"
            android:fontFamily="@font/latobold"
            android:gravity="center"
            android:paddingStart="25dp"
            android:paddingEnd="40dp"
            android:text="Sign in with Facebook"
            android:textAlignment="center"
            android:textColor="#1D212D"
            android:textSize="20sp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <EditText
            android:id="@+id/textView3"
            android:layout_width="373dp"
            android:layout_height="65dp"
            android:layout_marginStart="19dp"
            android:layout_marginTop="195dp"
            android:background="@drawable/rect3"
            android:drawableStart="@drawable/lock"
            android:drawableEnd="@drawable/passhide"
            android:drawablePadding="15dp"
            android:ems="10"
            android:fontFamily="@font/latoregular"
            android:hint="Enter your password"
            android:inputType="textPassword"
            android:paddingStart="25dp"
            android:paddingEnd="25dp"
            android:textAlignment="textStart"
            android:textColor="#1D212D"
            android:textSize="20sp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:id="@+id/headline3"
            android:layout_width="124dp"
            android:layout_height="21dp"
            android:layout_alignParentStart="true"
            android:layout_alignParentTop="true"
            android:layout_alignParentEnd="true"
            android:layout_alignParentBottom="true"
            android:layout_marginStart="253dp"
            android:layout_marginTop="273dp"
            android:fontFamily="@font/latobold"
            android:lineSpacingExtra="3sp"
            android:onClick="forgotpass"
            android:text="Forgot Password?"
            android:textColor="#1D212D"
            android:textSize="@dimen/text_size_small"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:id="@+id/headline5"
            android:layout_width="35dp"
            android:layout_height="19dp"
            android:layout_alignParentStart="true"
            android:layout_alignParentTop="true"
            android:layout_alignParentEnd="true"
            android:layout_alignParentBottom="true"
            android:layout_marginStart="192dp"
            android:layout_marginTop="463dp"
            android:fontFamily="@font/latobold"
            android:text="OR"
            android:textColor="#1D212D"
            android:textSize="15sp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:id="@+id/headline2"
            android:layout_width="317dp"
            android:layout_height="40dp"
            android:layout_alignParentStart="true"
            android:layout_alignParentTop="true"
            android:layout_alignParentEnd="true"
            android:layout_alignParentBottom="true"
            android:layout_marginStart="47dp"
            android:layout_marginTop="34dp"
            android:fontFamily="@font/latoheavy"
            android:gravity="center_horizontal|center_vertical"
            android:lineSpacingExtra="3sp"
            android:text="Login"
            android:textColor="#1D212D"
            android:textSize="26sp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:id="@+id/textView7"
            android:layout_width="292dp"
            android:layout_height="41dp"
            android:layout_marginStart="60dp"
            android:layout_marginTop="409dp"
            android:fontFamily="@font/latoregular"
            android:gravity="center"
            android:text="Don't have an account? Sign Up"
            android:textColor="#1D212D"
            android:textSize="@dimen/text_size_medium"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <ImageButton
            android:id="@+id/imageButton2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="34dp"
            android:layout_marginEnd="356dp"
            android:background="@drawable/backbuttom"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <View
            android:id="@+id/view2"
            android:layout_width="168dp"
            android:layout_height="2dp"
            android:layout_marginStart="224dp"
            android:layout_marginTop="470dp"
            android:background="@drawable/line"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <View
            android:id="@+id/view"
            android:layout_width="168dp"
            android:layout_height="2dp"
            android:layout_marginStart="17dp"
            android:layout_marginTop="470dp"
            android:background="@drawable/line"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />


        <androidx.constraintlayout.widget.Group
            android:id="@+id/group"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:constraint_referenced_ids="textView5,textView6,textView9,textView10,textView12,textView13,textView3,headline3,headline5,headline2,textView7,imageButton2,view" />


    </androidx.constraintlayout.widget.ConstraintLayout>
</ScrollView>